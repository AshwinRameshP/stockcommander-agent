{
  "name": "intelligent-inventory-replenishment",
  "version": "1.0.0",
  "description": "AI-powered inventory replenishment system using AWS CDK",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --watchAll=false",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:integration": "jest --testPathPattern=integration --watchAll=false",
    "test:smoke": "jest --testPathPattern=smoke --watchAll=false",
    "test:e2e": "jest --testPathPattern=e2e --watchAll=false",
    "test:performance": "jest --testPathPattern=performance --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "cdk": "cdk",
    "cdk:deploy": "cdk deploy",
    "cdk:destroy": "cdk destroy",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "deploy": "./scripts/deploy.sh",
    "deploy:dev": "./scripts/deploy.sh dev",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "rollback:infrastructure": "node scripts/rollback-infrastructure.js",
    "rollback:frontend": "node scripts/rollback-frontend.js",
    "monitor:deployment-health": "node scripts/monitor-deployment.js",
    "generate:deployment-report": "node scripts/generate-deployment-report.js",
    "frontend:install": "cd frontend && npm install",
    "frontend:start": "cd frontend && npm start",
    "frontend:build": "cd frontend && npm run build",
    "frontend:build:staging": "cd frontend && npm run build:staging",
    "frontend:build:production": "cd frontend && npm run build:production",
    "frontend:test": "cd frontend && npm test -- --watchAll=false",
    "frontend:lint": "cd frontend && npm run lint"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "aws-cdk": "2.100.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.400.0",
    "@aws-sdk/client-bedrock-runtime": "^3.400.0",
    "@aws-sdk/client-cloudwatch": "^3.913.0",
    "@aws-sdk/client-dynamodb": "^3.400.0",
    "@aws-sdk/client-eventbridge": "^3.400.0",
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/client-sagemaker": "^3.400.0",
    "@aws-sdk/client-sagemaker-runtime": "^3.400.0",
    "@aws-sdk/client-sns": "^3.400.0",
    "@aws-sdk/client-sqs": "^3.400.0",
    "@aws-sdk/lib-dynamodb": "^3.913.0",
    "@aws-sdk/s3-request-presigner": "^3.400.0",
    "aws-cdk-lib": "2.100.0",
    "constructs": "^10.0.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "inventory",
    "ai",
    "bedrock"
  ],
  "author": "",
  "license": "MIT"
}
